class Temperature{
	private double temp;
	private char c='C';
	public Temperature(char c) {
		this.temp=0;
		this.c=c;
	}
	public Temperature(double temp) {
		this.temp=temp;
	}
	public Temperature(double temp, char c) {
		this.temp=temp;
		this.c=c;
	}
	public Temperature() {
		this.temp=0;
	}
	
}
public class Ex1 {

}
