import java.util.Vector;

class Person{
	  Gender gender;
	  public Person(Gender gender) {
		  this.gender=gender;
	  }
	public String toString()
	{
		switch (gender) {
		case BOY:
			return "BOY";
		default:
			return "GIRL";
		}
}
}
public class DragonLaunch {
	static Vector<Person> v1= new Vector<Person>();
	
	public static void kindap(Person p) {
		v1.addElement(p);
	}
	public static void willDragonEatorNot() {
		if (v1.size()%2!=0) {
			System.out.println("YES");
		}
		else {
		for (int i=0; i<v1.size(); i++) {
			System.out.println(i);
			if (v1.get(i).gender==Gender.BOY && v1.get(i+1).gender==Gender.GIRL) {
				v1.remove(i);
				v1.remove(i);
				i=0;
				System.out.println(i);
				}	
			}
		if (v1.size()!=0) {
			System.out.println("YES");
		}
		else {
			System.out.println("NO");
		}
		}
		System.out.println(v1.toString());
	}
	public static void main(String[] args) {
		Person p1 = new Person(Gender.BOY);
		Person p2 = new Person(Gender.GIRL);
		Person p3 = new Person(Gender.BOY);
		Person p4 = new Person(Gender.GIRL);
		kindap(p1);
		kindap(p2);
		
		kindap(p3);
		kindap(p4);
		willDragonEatorNot();
		
	}

}
